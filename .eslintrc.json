{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "strict": 0,
    "max-len": [2, 200, 2, {"ignoreUrls": true}],
    "no-nested-ternary": 0,
    "object-property-newline": 0,
    "react/jsx-filename-extension": 0
  },
  "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true
      }
  }
}
